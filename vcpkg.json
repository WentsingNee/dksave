{
  "name": "dksave",
  "version-string": "2024.9.1",
  "builtin-baseline": "91d888703f251c13111c1b889be1f350c4ceb7ab",
  "dependencies": [
    {
      "name": "ctre",
      "version>=": "3.9.0"
    },
    {
      "name": "fmt",
      "version>=": "11.0.2"
    },
    {
      "name": "kerbal",
      "version>=": "2024.8.1"
    },
    {
      "name": "opencv4",
      "version>=": "4.8.0#22",
      "default-features": false,
      "features": [
        "png"
      ]
    },
    {
      "name": "yaml-cpp",
      "version>=": "0.8.0#1"
    }
  ],
  "features": {
    "k4a": {
      "description": "enable k4a cameras support",
      "dependencies": [
        {
          "name": "azure-kinect-sensor-sdk",
          "version>=": "1.4.1#6"
        }
      ]
    },
    "ob": {
      "description": "enable ob cameras support",
      "dependencies": [
        {
          "name": "ffmpeg",
          "version>=": "7.0.2#4",
          "default-features": false,
          "features": [
            "avcodec",
            "swresample",
            "swscale"
          ]
        }
      ]
    },
    "pcl": {
      "description": "enable pcl point cloud library support",
      "dependencies": [
        {
          "name": "pcl",
          "version>=": "1.14.1#1"
        }
      ]
    }
  }
}
